{
  "$schema": "../../../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/ui-components",
  "assets": [
    "./_index.scss",
    {
      "input": "src/styles",
      "glob": "**/*.scss",
      "output": "src/styles"
    }
  ],
  "lib": {
    "entryFile": "src/public-api.ts",
    "styleIncludePaths": [
      "src/styles"
    ]
  }
}
