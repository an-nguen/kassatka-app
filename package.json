{
  "name": "kassatka-app",
  "packageManager": "yarn@3.4.1",
  "private": true,
  "workspaces": [
    "packages/*",
    "packages/client/projects/*"
  ],
  "license": "MIT",
  "scripts": {
    "build:core": "yarn workspace @kassatka/core build",
    "dev:server": "yarn workspace server start:dev",
